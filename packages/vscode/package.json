{
  "name": "macaron-vscode",
  "displayName": "macaron-vscode",
  "version": "0.0.1",
  "description": "Macaron editor for VS Code",
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "build": "tsc -p ./",
    "dev": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "yarn run compile"
  },
  "contributes": {
    "commands": [
      {
        "command": "macaron-vscode.helloWorld",
        "title": "Hello World"
      }
    ],
    "customEditors": [
      {
        "viewType": "macaron.macaronFile",
        "displayName": "Macaron",
        "selector": [
          {
            "filenamePattern": "*.macaron"
          }
        ]
      }
    ]
  },
  "activationEvents": [
    "onCommand:macaron-vscode.helloWorld",
    "onCustomEditor:macaron.macaronFile"
  ],
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "17.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^10.0.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "comlink": "^4.3.1",
    "globby": "^13.1.1",
    "lodash": "^4.17.21"
  }
}
